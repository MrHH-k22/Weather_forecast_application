{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "src/app/_layout.jsx",
    "src/app/index.jsx",
    "constants/index.js",
    "src/components/footerTab.jsx",
    "src/features/account/signUp.jsx",
    "src/features/account/signIn.jsx",
    "src/features/account/account.jsx",
    "src/app/cities/[id].jsx"
  ]
}
